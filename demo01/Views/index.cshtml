@model List<User>

<h1>Users</h1>

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Username</th>
            <th>Email</th>
            <th>Password</th>
            <th>Created At</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.ID</td>
                <td>@user.Username</td>
                <td>@user.Email</td>
                <td>@user.Password</td>
                <td>@user.CreatedAt</td>
            </tr>
        }
    </tbody>
</table>
